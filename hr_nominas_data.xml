<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="hr_payroll_sequence_type" model="ir.sequence.type">
            <field name="name">Payroll sequence</field>
            <field name="code">hr.nomina</field>
        </record>

        <record id="hr_payroll_sequence" model="ir.sequence">
            <field name="name">Payroll sequence</field>
            <field name="code">hr.nomina</field>
            <field name="prefix">NOM/%(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</openerp>